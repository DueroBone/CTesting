{
  "tasks": [
    {
      "label": "Build All",
      "type": "shell",
      "command": "/usr/bin/make",
      // "dependsOn": "OBJ dump",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ],
      "detail": "Task generated by Debugger."
    },
    {
      "label": "Build All MemCheck",
      "type": "shell",
      "command": "/usr/bin/make",
      "args": [
        "MEMCHECK=1"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      // "dependsOn": "OBJ dump",
      "problemMatcher": [
        "$gcc"
      ],
      "detail": "Task generated by Debugger."
    },
    {
      "label": "OBJ dump",
      "type": "shell",
      "command": "/usr/bin/objdump",
      "args": [
        "-d",
        "${workspaceFolder}/Enigma"
      ],
      "options": {
        "stdout": "${workspaceFolder}/AsmEnigma.asm"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": [
        "$gcc"
      ],
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}