{
    "tasks": [
    {
      "label": "Build All",
      "type": "shell",
      "command": "/usr/bin/gcc",
      "args": [
        "-O0",
        "-Wall",
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "*.c",
        "-o",
        "${workspaceFolder}/Enigma"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ],
      "detail": "Task generated by Debugger."
    },
    {
      "label": "Build All Optimized",
      "type": "shell",
      "command": "/usr/bin/gcc",
      "args": [
        "-O2",
        "-Wall",
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "*.c",
        "-o",
        "${workspaceFolder}/Enigma"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": [
        "$gcc"
      ],
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}